<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
    	<!-- SQL 결과의 컬럼명(columnName)‑값(value) 쌍을 EgovMap에 담아 돌려주는 역할 -->
		<typeAlias alias="egovMap" type="org.egovframe.rte.psl.dataaccess.util.EgovMap"/>
		<typeAlias alias="userVO" type="egovframework.third.homework.service.UserVO"/>
		<typeAlias alias="surveyVO" type="egovframework.third.homework.service.SurveyVO"/>
		<typeAlias alias="questionVO" type="egovframework.third.homework.service.QuestionVO"/>
		<typeAlias alias="qitemVO" type="egovframework.third.homework.service.QitemVO"/>
		<typeAlias alias="qimageVO" type="egovframework.third.homework.service.QimageVO"/>
    </typeAliases>
</configuration>